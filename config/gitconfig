[alias]
  br = branch
  cm = commit -m
  co = checkout
  d = diff
  gl = config -l --global
  last = log -1 HEAD
  ll = log --oneline
  p = push
  pd = !git pull -p && git fetch --prune && (git branch -vv | grep ': gone]' | awk '{print $1}' | xargs git branch -D)
  rv = remote -v
  se = !git rev-list --all | xargs git grep -F
  st = status -bs
  unstage = reset HEAD --
[core]
  editor = vim
  autocrlf = false
[init]
  defaultBranch = main
[user]
  email = jim.salyer@gmail.com
  name = Jim Salyer
